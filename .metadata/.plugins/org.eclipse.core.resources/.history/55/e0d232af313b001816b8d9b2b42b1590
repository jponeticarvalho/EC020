typedef struct tag_Timer
{
	volatile uint32_t uiTimerCount;
} ttag_Timer;

ttag_Timer tagTimer;

void SysTick_handler(void)
{
	tagTimer.uiTimerCount++;
}

void Timer_init(void)
{
	tagTimer.uiTimerCount = 0;


}

