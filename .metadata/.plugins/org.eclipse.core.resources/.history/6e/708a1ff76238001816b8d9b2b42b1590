#include "Display_Ctrl/Oled_Ctrl/Oled_Ctrl.h"
#include "Display_Ctrl/Oled_Ctrl/Ssp_Ctrl/Ssp_Ctrl.h"

int main (void)
{
//	command_init();
	Ssp_Ctrl_Init();
	oled_init();
	oled_clearScreen(OLED_COLOR_BLACK);
	oled_putString(1,  1,(uint8_t *)"               ",OLED_COLOR_WHITE, OLED_COLOR_BLACK);
	oled_putString(1,  9,(uint8_t *)"Grupo de EC020:",OLED_COLOR_WHITE, OLED_COLOR_BLACK);
	oled_putString(1, 17,(uint8_t *)"Fer Avelar,    ",OLED_COLOR_WHITE, OLED_COLOR_BLACK);
	oled_putString(1, 17,(uint8_t *)"Joao Pedro,    ",OLED_COLOR_WHITE, OLED_COLOR_BLACK);
	oled_putString(1, 17,(uint8_t *)"Karla,         ",OLED_COLOR_WHITE, OLED_COLOR_BLACK);

	while(1)
	{
//		command_processLoop();
//		display_processLoop();
	}
}
